{
    "name": "timoschinkel/codeowners-cli",
    "description": "A cli interface for the codeowners library",
    "type": "project",
    "license": "Apache-2.0",
    "require": {
        "php": "^8.1",
        "symfony/console": "^6.0|^7.0",
        "timoschinkel/codeowners": "^2.0",
        "symfony/finder": "^6.0|^7.0"
    },
    "bin": ["bin/codeowners"],
    "autoload": {
        "psr-4": {
            "CodeOwners\\Cli\\": "src",
            "CodeOwners\\Cli\\Tests\\": "tests"
        }
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.5",
        "vimeo/psalm": "~6.4.0",
        "phpunit/phpunit": "^9.4",
        "mikey179/vfsstream": "^1.6",
        "phpspec/prophecy-phpunit": "^2.0"
    }
}
