{
    "name": "timoschinkel/codeowners-cli",
    "description": "A cli interface for the codeowners library",
    "type": "project",
    "license": "Apache-2.0",
    "require": {
        "php": "^7.2",
        "symfony/console": "^5.0",
        "timoschinkel/codeowners": "^1.1.0",
        "symfony/finder": "^5.0"
    },
    "bin": ["bin/codeowners"],
    "autoload": {
        "psr-4": {
            "CodeOwners\\Cli\\": "src",
            "CodeOwners\\Cli\\Tests\\": "tests"
        }
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.5",
        "vimeo/psalm": "^4.2",
        "phpunit/phpunit": "^8.5",
        "mikey179/vfsstream": "^1.6"
    }
}
